{
  "name": "firsttable/firsttable",
  "language-level": "8.1",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:FirstTable/silverstripe-bananas.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:FirstTable/silverstripe-dataobjectcontroller.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:FirstTable/silverstripe-twitter-bootstrap.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:FirstTable/silverstripe-responsive-images.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:FirstTable/silverstripe-logos.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:FirstTable/silverstripe-gridfieldextensions.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:FirstTable/silverstripe-addressable.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:FirstTable/silverstripe-graphql-jwt.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:FirstTable/phpQuery.git",
      "reference": "dev-php8-compat"
    },
    {
      "type": "vcs",
      "url": "git@github.com:FirstTable/silverstripe-graphql-devtools.git"
    }
  ],
  "require": {
    "php": "^8.0",
    "silverstripe/recipe-core": "4.13.*",
    "silverstripe/assets": "1.13.*",
    "silverstripe/config": "1.6.*",
    "silverstripe/campaign-admin": "1.13.*",
    "silverstripe/versioned-admin": "1.13.*",
    "silverstripe/cms": "4.13.*",
    "silverstripe/errorpage": "^1.10",
    "silverstripe/graphql": "4.3.*",
    "silverstripe/reports": "4.7.*",
    "silverstripe/siteconfig": "4.7.*",
    "silverstripe/versioned": "1.13.*",
    "silverstripe/asset-admin": "1.13.*",
    "silverstripe/subsites": "2.5.*",
    "silverstripe/blog": "^3.0",
    "silverstripe/graphql-devtools": "dev-firsttable",
    "silverstripe/userforms": "^5.11",
    "silverstripe/recaptcha": "dev-master",
